var swiper = new Swiper(".mySwiper", {
	    loop: true,
	    navigation: {
	        nextEl: ".swiper-button-next",
	        prevEl: ".swiper-button-prev",
	    },
	    autoplay: {
	        delay: 5000, // 5초마다 슬라이드가 넘어갑니다.
	        disableOnInteraction: false, // 사용자가 슬라이드를 조작해도 자동 재생이 멈추지 않도록 설정
	    },
	});
	